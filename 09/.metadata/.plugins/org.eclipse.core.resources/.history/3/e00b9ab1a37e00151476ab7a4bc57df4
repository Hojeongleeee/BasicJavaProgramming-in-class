import java.util.Scanner;

public class Person implements Comparable{

	Scanner scan = new Scanner(System.in);
	
	private String name;
	private double height;
	private double weight;
	private int scoreBMI;	
	
	
	Person(){
		System.out.print("이름:");
		this.name=setName();
		System.out.print("키:");
		this.height=setHeight();
		System.out.print("몸무게:");
		this.weight=setWeight();
		this.scoreBMI=(int)(weight/((height-100)*0.9)*100); //소수점이하 X
	}
	

	@Override
	public int compareTo(Object obj) {
		Person person = (Person)obj;
		//this.scoreBMI(person)
		
		if(this.scoreBMI>=getScoreBMI(person)) //this>person
			return 1;
		else  //자료 정렬에서 return값이 1이아닌이상 의미없음
			return 0;
	}

	//getter
	public int getScoreBMI(Person person) {
		return person.scoreBMI;
	}
	
	public String getName(Person person) {
		return person.name;
	}
	
	public double getWeight(Person person) {
		return person.weight;
	}
	
	public double getHeight(Person person) {
		return person.height;
	}
	
	
	//setter
	private double setWeight() {
		double weight=scan.nextDouble();
		return weight;
	}

	private double setHeight() {
		double height=scan.nextDouble();
		return height;
	}
	
	private String setName() {
		String name=scan.nextLine();
		return name;
	}




}
