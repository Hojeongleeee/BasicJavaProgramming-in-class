
class Truck extends Car {
	int wheels; //바퀴수 4, 6, 10
	int load; //1톤, 5톤, 10톤
	
	//op1, op2를 매개변수로 한 AutoMobile 클래스의 객체 생성자
	Truck(String a, int b, String op1, int op2){
		super(a,b);
		wheels = Integer.parseInt(op1);
		load = op2;
	}
	
	void drive(){
		System.out.println("최고 80Km/h 제한속도");
	}
	
}
